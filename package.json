{
  "name": "vidav-front",
  "private": true,
  "scripts": {
    "dev": "yarn codegen:generate && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "codegen:generate": "apollo client:codegen --excludes=node_modules/* --target typescript --tagName=gql --outputFlat src/types/__generated__ && rm src/types/__generated__/globalTypes.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@chakra-ui/react": "^1.8.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^5",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.0.0",
    "@types/node": "17.0.14",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "apollo": "^2.33.9",
    "commitlint": "^16.1.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "typescript": "4.5.5"
  }
}
